{
	"name" : "Python3 Search",
	"manifest_version": 2,
	"description" : "Adds support to the omnibox to search Python 3.0+ reference documentation.",
	"background" : {
		"scripts" : [ "String.js", "predefined.js", "background.js"]
	},
	"icons" : {
		"128" : "icon128.png",
		"32" : "icon32.png",
		"16" : "icon16.png"
	},
	"omnibox" : {
		"keyword" : "py3"
	},
	"permissions" : [ 
		"tabs", 
		"*://docs.python.org/", 
		"*://docs.scipy.org/", 
		"*://docs.sympy.org/", 
		"*://matplotlib.sourceforge.net/", 
		"*://pylonshq.com/"
	],
	"version": "3.0.0"
}
